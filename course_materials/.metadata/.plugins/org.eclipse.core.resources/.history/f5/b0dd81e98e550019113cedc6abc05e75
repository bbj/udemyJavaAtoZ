public class App {

	/**
	 * Lambda expression
	 * 
	 **/

	public static void main(String[] args) {

		/**
		 * Lambda expression example
		 * 
		 **/

		// simplified lambda expression
		Sum s2 = (i, i2) -> i+i2;

		System.out.println(s2.adding(5, 3));
	}
}
